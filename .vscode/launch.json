{
  "version": "0.2.0",
  "configurations": [
      {
          "name": "Flask",
          "type": "python",
          "request": "launch",
          "stopOnEntry": false,
          "program": "${workspaceRoot}/app/backend/app.py",
          "env": {
              "FLASK_APP": "${workspaceRoot}/app.py"
          },
          "args": [
              "run"
          ],
          "envFile": "${workspaceFolder}/.env"
      }
  ]
}